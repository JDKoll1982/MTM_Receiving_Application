{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build-x64",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"/p:Platform=x64",
				"/p:Configuration=Debug"
			],
			"problemMatcher": [
				"$msCompile"
			],
			"group": "build",
			"presentation": {
				"echo": false,
				"reveal": "never",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": false,
				"clear": true
			}
		}
	]
}